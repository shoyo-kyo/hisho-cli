<!DOCTYPE html>
<html {% for k,v of @htmlAttr : %}{%- k %}="{%- v %}"{% end %}>
<head {% for k,v of @headAttr : %}{%- k %}="{%- v %}"{% end %}>
	{%- @head %}
	<link rel="stylesheet" href="../css/{%- @name %}.css" />
	{% for item in @import : %}
		<link rel="import" href="{%- item %}">
	{% end %}
</head>
<body {% for k,v of @bodyAttr : %}{%- k %}="{%- v %}"{% end %}>

{%- @body %}

<!--SCRIPT-->
<script type="text/javascript" src="../js/{%- @name %}.js">[_]</script>
<!--/SCRIPT-->

</body>
</html>
